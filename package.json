{
  "name": "diary",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"server,client\" \"yarn server dev\" \"yarn client dev\"",
    "server": "yarn workspace server",
    "client": "yarn workspace client",
    "audit:ci": "audit-ci --high --config=./yarn-audit-ci.config.json"
  },
  "workspaces": [
    "workspaces/*"
  ],
  "devDependencies": {
    "@types/node": "^17.0.17",
    "audit-ci": "^5.1.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "prettier": "^2.6.0",
    "source-map-support": "^0.5.21",
    "typescript": "^4.6.2"
  },
  "packageManager": "yarn@3.2.0"
}
