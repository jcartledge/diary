{
  "name": "@diary/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@diary/shared": "workspace:*",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-resolvers": "^2.6.4",
    "apollo-server": "^3.8.2",
    "apollo-server-express": "^3.10.0",
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "http": "^0.0.1-security",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/pg": "8.6.5",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "nodemon": "2.0.19",
    "supertest": "6.2.4",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "vitest": "0.21.1",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "scripts": {
    "build": "webpack",
    "codegen": "graphql-codegen",
    "coverage": "vitest run --coverage",
    "dev:codegen": "graphql-codegen --watch",
    "dev:server": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "dev": "concurrently -n \"server,codegen\" \"yarn dev:server\" \"yarn dev:codegen\"",
    "lint": "eslint src/",
    "test": "vitest"
  }
}
