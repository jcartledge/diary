{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-resolvers": "^2.6.4",
    "apollo-server": "^3.8.2",
    "concurrently": "^7.2.1",
    "graphql": "^16.5.0",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/pg": "8.6.5",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "nodemon": "2.0.19",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "vitest": "0.20.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "scripts": {
    "build": "webpack",
    "codegen": "graphql-codegen",
    "coverage": "vitest run --coverage",
    "dev:codegen": "graphql-codegen --watch",
    "dev:server": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "dev": "concurrently -n \"server,codegen\" \"yarn dev:server\" \"yarn dev:codegen\"",
    "lint": "eslint src/",
    "test": "vitest"
  }
}
