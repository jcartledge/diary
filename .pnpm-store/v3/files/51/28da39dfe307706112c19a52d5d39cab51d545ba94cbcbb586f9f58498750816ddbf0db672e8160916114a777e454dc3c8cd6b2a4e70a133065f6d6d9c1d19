export declare function isPromise<T>(value: unknown): value is Promise<T>;
export declare function ok(value: unknown, message?: string): asserts value;
export declare function ok<T>(value: unknown, message?: string): asserts value is T;
export declare function isPromiseRejectedResult(value: PromiseSettledResult<unknown>): value is PromiseRejectedResult;
export declare function like<T>(value: unknown): value is T;
